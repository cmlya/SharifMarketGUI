<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ShowItems">
   <children>
      <Pane fx:id="pane" layoutX="42.0" layoutY="110.0" prefHeight="450.0" prefWidth="734.0">
         <children>
            <VBox fx:id="vBox" layoutX="15.0" layoutY="23.0" prefHeight="406.0" prefWidth="695.0">
               <children>
                  <TableView fx:id="itemTableView" prefHeight="296.0" prefWidth="327.0">
                    <columns>
                      <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Item Name" />
                      <TableColumn fx:id="codeColumn" prefWidth="75.0" text="Item Code" />
                        <TableColumn fx:id="sellingPriceColumn" prefWidth="75.0" text="Price" />
                        <TableColumn fx:id="inStockColumn" prefWidth="75.0" text="In Stock" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
         </children></Pane>
      <Button layoutX="722.0" layoutY="45.0" mnemonicParsing="false" onAction="#exit" text="EXIT">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="590.0" layoutY="45.0" mnemonicParsing="false" onAction="#mainMenu" text="MAIN MENU">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="374.0" layoutY="64.0" mnemonicParsing="false" onAction="#showItems" text="DISPLAY" />
   </children>
</AnchorPane>
