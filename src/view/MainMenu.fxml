<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainMenu">
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0">
         <children>
            <Label layoutX="292.0" layoutY="83.0" text="MAIN MENU">
               <font>
                  <Font size="38.0" />
               </font>
            </Label>
            <Button layoutX="670.0" layoutY="58.0" mnemonicParsing="false" onAction="#exit" text="EXIT">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button layoutX="71.0" layoutY="500.0" mnemonicParsing="false" onAction="#signOut" text="SIGN OUT">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button layoutX="246.0" layoutY="249.0" mnemonicParsing="false" onAction="#showItems" text="Show Items">
               <font>
                  <Font size="48.0" />
               </font>
            </Button>
            <MenuButton layoutX="621.0" layoutY="98.0" mnemonicParsing="false" text="Settings">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#changePassword" text="Change password" />
              </items>
               <font>
                  <Font size="15.0" />
               </font>
            </MenuButton>
         </children>
      </Pane>
   </children>
</AnchorPane>
